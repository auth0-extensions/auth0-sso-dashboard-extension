    "jwks-rsa": "^1.1.1",
    "jwt-decode": "^2.0.1",
    "lodash": "^4.8.2",
    "lru-memoizer": "1.10.0",
    "moment": "^2.13.0",
    "morgan": "^1.7.0",
    "ms": "^0.7.2",
    "nconf": "^0.8.4",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.7",
    "react-s-alert": "^1.2.0",
    "redux": "^3.6.0",
    "redux-form": "^4.2.2",
    "redux-logger": "^2.7.4",
    "redux-promise-middleware": "^4.1.0",
    "redux-thunk": "^2.1.0",
    "request": "^2.34.0",
    "request-promise": "^2.0.1",
    "reselect": "^2.5.4",
    "semver": "^5.1.0",
    "superagent": "^3.5.2",
