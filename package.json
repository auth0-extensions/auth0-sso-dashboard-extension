{
  "name": "auth0-sso-dashboard",
  "version": "0.1.0",
  "description": "SSO Dashboard",
  "engines": {
    "node": "6.2.0"
  },
  "main": "server.js",
  "scripts": {
    "clean": "rimraf dist",
    "client:build": "npm run clean && cross-env NODE_ENV=production webpack --config ./build/webpack/config.prod.js --colors -p",
    "serve:dev": "cross-env NODE_ENV=development nodemon --ignore assets/app/ --ignore build/webpack/ --ignore client/ --ignore node_modules/ ./build/webpack/server.js",
    "serve:prod": "cross-env NODE_ENV=production node index.js",
    "test": "cross-env NODE_ENV=test mocha -R spec --compilers js:babel-core/register --timeout 5000 $(find ./tests -name *.tests.js) tests/runner.js",
    "test:watch": "cross-env NODE_ENV=test mocha -R spec --compilers js:babel-core/register --recursive --timeout 5000 $(find ./tests -name *.tests.js) --watch tests/runner.js",
    "extension:build": "webpack -p --config ./build/extension/webpack.config.js",
    "extension:size": "cross-env NODE_ENV=production webpack -p --config ./build/extension/webpack.config.js --json > ./build/extension/bundle-size.json && node ./build/extension/bundle-size.js",
    "extension:bundle": "cp ./dist/auth0-sso-dashboard.extension.$npm_package_version.js ./build/bundle.js",
    "win-extension:bundle": "copy dist\\auth0-sso-dashboard.extension.%npm_package_version%.js build\\bundle.js /y",
    "build": "npm run clean && npm run client:build && npm run extension:build && npm run extension:bundle",
    "win-build": "cross-env NODE_ENV=production && npm run client:build && npm run extension:build && npm run win-extension:bundle",
    "tag": "git tag $npm_package_version && git push --tags"
  },
  "keywords": [
    "auth0"
  ],
  "author": "Sandrino Di Mattia",
  "license": "MIT",
  "dependencies": {
    "async": "^1.5.2",
    "auth0": "^2.1.0",
    "auth0-extension-express-tools": "^0.2.0",
    "auth0-extension-tools": "^0.13.0",
    "axios": "^0.13.1",
    "babel": "^6.5.2",
    "babel-core": "^6.9.1",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.9.2",
    "bluebird": "^3.4.0",
    "body-parser": "^1.15.1",
    "ejs": "^2.4.2",
    "express": "^4.13.4",
    "express-jwt": "^3.4.0",
    "glob": "^7.0.3",
    "json-loader": "^0.5.4",
    "jsonwebtoken": "^7.1.7",
    "jwks-rsa": "^1.0.0",
    "lodash": "^4.8.2",
    "lru-memoizer": "^1.5.0",
    "moment": "^2.13.0",
    "morgan": "^1.7.0",
    "ms": "^0.7.1",
    "nconf": "^0.8.4",
    "react-s-alert": "^1.1.4",
    "request": "^2.34.0",
    "request-promise": "^2.0.1",
    "semver": "^5.1.0",
    "uuid": "^2.0.2",
    "webtask-tools": "^2.2.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "chai": "^3.5.0",
    "classnames": "^2.2.5",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "history": "^2.1.2",
    "immutable": "^3.8.1",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "jwt-decode": "^2.0.1",
    "mocha": "^3.0.0",
    "nock": "^8.0.0",
    "nodemon": "^1.9.2",
    "postcss-focus": "^1.0.0",
