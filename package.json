    "ngrok": "2.2.4",
    "nock": "^9.0.2",
    "nodemon": "^1.11.0",
    "nyc": "^13.1.0",
    "open": "^6.0.0",
    "postcss-focus": "^3.0.0",
    "postcss-loader": "^2.1.3",
    "postcss-reporter": "^5.0.0",
    "postcss-simple-vars": "^4.1.0",
    "raw-loader": "^0.5.1",
